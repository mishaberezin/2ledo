{
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "open -a Simulator & expo start --ios",
    "eject": "expo eject",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/samples": "~3.0.0",
    "@expo/vector-icons": "^10.0.1",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "expo": "^33.0.0",
    "expo-asset": "^5.0.0",
    "expo-cli": "^3.0.8",
    "expo-constants": "^5.0.0",
    "expo-font": "^5.0.0",
    "expo-image-picker": "^5.0.2",
    "expo-linear-gradient": "^6.0.0",
    "expo-web-browser": "^5.0.0",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-elements": "^1.1.0",
    "react-native-gesture-handler": "^1.2.2",
    "react-native-reanimated": "^1.0.1",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "^0.11.4",
    "react-navigation": "^3.11.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "babel-preset-expo": "^5.1.0",
    "eslint": "^6.1.0",
    "eslint-plugin-jest": "^22.14.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-native": "^3.7.0",
    "husky": "^3.0.2",
    "jest-expo": "^33.0.0",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2"
  },
  "babel": {
    "presets": [
      "babel-preset-expo"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "react-native/react-native": true,
      "jest/globals": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jest/recommended"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "react-native",
      "jest"
    ],
    "rules": {
      "react/prop-types": [
        0
      ],
      "react/display-name": [
        0
      ]
    }
  }
}
